
# uppgift 1
def kub(x):
    return x**3

sum = 0
for i in range(1,20,2):
    sum += kub(i)

print(sum)


# uppgift 2
def linjen(x1,y1,x2,y2):
    print((y1-y2)/(x1-x2),"x +",y1 - (((y1-y2)/(x1-x2))*x1))

linjen(0,-2,1,1)


# uppgift 3
b = int(input("basen = "))
h = int(input("höjden = "))

def area(b,h):
    return b*h

def omkrets(b,h):
    return (2*b)+(2*h)

print("\narean är",area(b,h), "a.e.")
print("omkretsen är",omkrets(b,h),"l.e.\n")
